    <script>
    const imageInput = document.getElementById('image-input-cabinet');
    const imagePreview = document.getElementById('image-preview-cabinet');
    const cabinet_image_download = document.getElementById('cabinet_image_download');
      
      imageInput.addEventListener('change', () => {
