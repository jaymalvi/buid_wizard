<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Build Wizard</title>

    <!-- Google Font: Source Sans Pro -->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback"
    />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/plugins/fontawesome-free/css/all.min.css" />
    <!-- SweetAlert2 -->
    <link rel="stylesheet" href="/plugins/sweetalert2/sweetalert2.min.css" />
    <!-- Toastr -->
    <link rel="stylesheet" href="/plugins/toastr/toastr.min.css" />
    <!-- Theme style -->
    <link rel="stylesheet" href="/dist/css/adminlte.min.css" />
  </head>
  <body class="hold-transition sidebar-mini">
    <div class="wrapper">
      <%- include('../../partials/top-nav'); %> 
      <%- include('../../partials/sidebar'); %>
      
      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <div class="container-fluid">
            <div class="row mb-2">
              <div class="col-sm-6">
                <h1>Add Items</h1>
              </div>
              <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                  <li class="breadcrumb-item">
                    <a href="/sup_dashborad">Back</a>
                  </li>
                </ol>
              </div>
            </div>
          </div>
          <!-- /.container-fluid -->
        </section>

        <!-- Main content -->
        <!-- Main content -->
        <section class="content">
          <div class="container-fluid">
            <div class="row">
              <!-- left column -->
              <div class="col-md-12">
                <!-- jquery validation -->
                <div class="card card-primary">
                  <form
                    id="quickForm" enctype="multipart/form-data"
                    action="<%= typeof data=='object' && data.item ? '/cabinet/update' : '/cabinet/add' %>"
                    method="post"
                  >
                    <div class="row">
                      <div class="card-body">
                        <div class="form-group">
                          <div class="form-group">
                            <label for="dropdownItem">Item</label>
                            <select
                              id="dropdownItem"
                              name="item"
                              class="form-control select2"
                              data-placeholder="Select a Item"
                              style="width: 100%"
                            >
                              <option value="">
                                --Please choose an Item--
                              </option>
                              <option value="cabinet" <%= typeof data=='object' && data.item == 'cabinet' ? 'selected' : '' %>>Cabinet</option>
                              <option value="motherboard" <%= typeof data=='object' && data.item == 'motherboard' ? 'selected' : '' %>>Motherboard</option>
                              <option value="cpu"<%= typeof data=='object' && data.item == 'cpu' ? 'selected' : '' %>>CPU</option>
                              <option value="ram"<%= typeof data=='object' && data.item == 'ram' ? 'selected' : '' %>>Ram</option>
                              <option value="colling"<%= typeof data=='object' && data.item == 'colling' ? 'selected' : '' %>>Colling System</option>
                              <option value="graphiccard"<%= typeof data=='object' && data.item == 'graphiccard' ? 'selected' : '' %>>Graphic Card</option>
                              <option value="powersupply"<%= typeof data=='object' && data.item == 'powersupply' ? 'selected' : '' %>>Power Supplies</option>
                              <option value="storage"<%= typeof data=='object' && data.item == 'storage' ? 'selected' : '' %>>Storage</option>
                              <option value="peripheral"<%= typeof data=='object' && data.item == 'peripheral' ? 'selected' : '' %>>
                                Peripheral Device
                              </option>
                            </select>

                            <!-- Cabinet related values -->
                            <div class="row" id="cabinet">
                              <div class="col-md-6">
                                <label for="Name">Name</label>
                                <input
                                  type="hidden"
                                  name="idCabinet"
                                  value="<%= typeof data=='object' && data.idCabinet ? data.idCabinet : '' %>"
                                />
                                <input
                                  type="hidden"
                                  name="supplierId"
                                  value="<%= typeof userData=='object' && userData.idSupplier ? userData.idSupplier : '' %>"
                                />
                                <input
                                  type="text"
                                  name="NameCabinet"
                                  class="form-control"
                                  id="company"
                                  value="<%= typeof data=='object' && data.NameCabinet? data.NameCabinet : '' %>"
                                  placeholder="Enter company name"
                                />
                              </div>
                              <div class="col-md-3">
                                <label for="Price">Price</label>
                                <input
                                  type="number"
                                  name="Pricecabinet"
                                  class="form-control"
                                  id="Price"
                                  value="<%= typeof data=='object' && data.Pricecabinet ? data.Pricecabinet : '' %>"
                                  placeholder="Enter price"
                                />
                              </div>
                              <div class="col-md-3">
                                <label for="quantity">Quantity</label>
                                <input
                                  type="number"
                                  name="quantityCabinet"
                                  class="form-control"
                                  id="company"
                                  value="<%= typeof data=='object' && data.quantityCabinet ? data.quantityCabinet : '' %>"
                                  placeholder="Enter quantity"
                                />
                              </div>
                              <div class="col-md-6">
                                <label for="image">Image URL</label>
                                <input
                                  type="file"
                                  name="cabinet_ImageURL"
                                  class="form-control"
                                  id="image-input-cabinet"
                                  value="<%= typeof data=='object' && data.cabinet_ImageURL ? data.cabinet_ImageURL : '' %>"
                                  placeholder="Enter Dimention of cabinet"
                                />
                              </div>

                              <div class="col-md-6">
                                <a href="" download  id="cabinet_image_download">
                                  <img src="/<%= typeof data=='object' && data.cabinet_ImageURL ? data.cabinet_ImageURL : '' %>" height="100" width="100" id="image-preview-cabinet" style="padding: 10px;">
                                </a>
                              </div>

                              <div class="col-md-6">
                                <label for="factor">Factor</label>
                                <input
                                  type="text"
                                  name="Form_Factor"
                                  class="form-control"
                                  id="factor"
                                  value="<%= typeof data=='object' && data.Form_Factor ? data.Form_Factor : '' %>"
                                  placeholder="Enter factor"
                                />
                              </div>
                              <div class="col-md-6">
