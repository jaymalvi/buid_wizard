                              <div class="col-md-6">
                                <label for="ethernetport">Ethernet Ports</label
                                ><br />
                                <div class="form-group">
                                  <div class="form-check">
                                    <input
                                      type="radio"
                                      name="ethernet_port"
                                      value=0 <%= typeof data=='object' && data.ethernet_port == 0 ? 'checked' : '' %>
                                    />&nbsp; <label>YES</label> &nbsp; &nbsp;
                                    &nbsp;

                                    <input
                                      type="radio"
                                      name="ethernet_port"
                                      value=1 <%= typeof data=='object' && data.ethernet_port == 1 ? 'checked' : '' %>
                                    />&nbsp;
